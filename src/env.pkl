import "@aws/util/props.pkl"

hidden STAGE = read("prop:STAGE")

hidden AWS_REGION: props.Region = 
  if (STAGE == "dev")
    "ap-northeast-1"
  else if (STAGE == "stg")
    "ap-northeast-1"
  else if (STAGE == "prod")
    "ap-northeast-1"
  else
    throw("Invalid stage: ${STAGE}")
hidden AWS_PROFILE: String = read("env:AWS_PROFILE")